            <Keywords name="Keywords7">&quot;distance:&quot; &quot;offset:&quot; &quot;depth:&quot; &quot;within:&quot; &quot;dsize:&quot; &quot;byte_test:&quot; &quot;byte_jump:&quot; &quot;byte_extract:&quot; &quot;nocase:&quot; &quot;isdataat:&quot; &quot;rpc:&quot; &quot;pcre:&quot; &quot;fast_pattern:&quot; &quot;startswith:&quot; &quot;replace:&quot; &quot;prefilter:&quot; &quot;nocase;&quot; &quot;fast_pattern;&quot;</Keywords>
